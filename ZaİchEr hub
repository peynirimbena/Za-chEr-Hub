      Enabled = true,
      Invite = "4TpX76cJ",
      RememberJoins = false
   },
   KeySystem = true,
   KeySettings = {
      Title = "Security Verification",
      Subtitle = "Join Discord for Key",
      Note = "Link copied! Paste it in your browser and check #keys channel.",
      FileName = "NoSaveKey_" .. tostring(math.random(1, 1000000)),
      SaveKey = false,
      GrabKeyFromSite = false,
      Key = {"zaicher1"}
   }
})

-- TABS
local HomeTab = Window:CreateTab("üè† Home", 4483362458)
local InkGameTab = Window:CreateTab("üñåÔ∏è Ink Game", 4483362458)
local BattlegroundsTab = Window:CreateTab("‚öîÔ∏è Battlegrounds", 4483362458) 
local ANYTab = Window:CreateTab("üõ†Ô∏è Universal", 4483362458)

-- ### HOME TAB ### --
HomeTab:CreateSection("User Information")
HomeTab:CreateLabel("Welcome, " .. game.Players.LocalPlayer.Name)
HomeTab:CreateLabel("Current Server: " .. game.JobId)

HomeTab:CreateSection("Social Media")
HomeTab:CreateButton({
   Name = "Copy Discord Link (Again)",
   Callback = function()
      setclipboard(discordLink)
      Rayfield:Notify({
         Title = "Copied!",
         Content = "Link has been copied to clipboard.",
         Duration = 3
      })
   end,
})

HomeTab:CreateSection("Hub Status")
HomeTab:CreateLabel("Status: Working / Undetected")
