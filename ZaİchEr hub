local discordLink = "https://discord.gg/4TpX76cJ" 
pcall(function() setclipboard(discordLink) end)

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Zaƒ∞chEr Hub | Premium",
   Icon = 0,
   LoadingTitle = "Zaƒ∞chEr Hub Loading...", 
   LoadingSubtitle = "by peynirimben2",
   ConfigurationSaving = { Enabled = false },
   Discord = { Enabled = true, Invite = "4TpX76cJ", RememberJoins = false },
   KeySystem = false
})

-- ### TABS ### --
local HomeTab = Window:CreateTab("üè† Home", 4483362458)
local CheatTab = Window:CreateTab("üî• Main Cheats", 4483362458)
local InkGameTab = Window:CreateTab("Ink Game Scripts", 4483362458)
local BattlegroundsTab = Window:CreateTab("Battlegrounds", 4483362458) 
local MuscleLegendTab = Window:CreateTab("Muscle Legend", 4483362458)
local ANYTab = Window:CreateTab("Universal", 4483362458)

-- ### 1. HOME TAB (DOPDOLU VERSƒ∞YON) ### --
HomeTab:CreateSection("üë§ User Information")
HomeTab:CreateLabel("Current Player: " .. game.Players.LocalPlayer.DisplayName .. " (@" .. game.Players.LocalPlayer.Name .. ")")
HomeTab:CreateLabel("Account Age: " .. game.Players.LocalPlayer.AccountAge .. " days")
HomeTab:CreateLabel("Membership: " .. tostring(game.Players.LocalPlayer.MembershipType):sub(21))

HomeTab:CreateSection("üìä Server & Network Stats")
local PingLabel = HomeTab:CreateLabel("Ping: Measuring...")
local FPSLabel = HomeTab:CreateLabel("FPS: Measuring...")
HomeTab:CreateLabel("Server Runtime: " .. math.floor(workspace.DistributedGameTime/60) .. " minutes")
HomeTab:CreateLabel("Job ID: " .. game.JobId)

-- Ger√ßek zamanlƒ± Ping ve FPS g√ºncelleme
task.spawn(function()
    while task.wait(1) do
        local ping = math.floor(game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue())
        PingLabel:Set("Ping: " .. ping .. "ms")
        
        -- FPS hesaplama (basit y√∂ntem)
        local fps = math.floor(1/task.wait())
        FPSLabel:Set("FPS: " .. fps)
    end
end)

HomeTab:CreateSection("üîó Community & Support")
HomeTab:CreateButton({
   Name = "Copy Discord Invite Link",
   Callback = function() 
      setclipboard(discordLink)
      Rayfield:Notify({Title = "Success", Content = "Discord Link Copied!", Duration = 3})
   end,
})

-- ### 2. MAIN CHEATS (FLY, NOCLIP, STATS, FPS) ### --
CheatTab:CreateSection("Movement & Collision")

CheatTab:CreateButton({
   Name = "Mobile Fly GUI (XNEOFF)",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))()
   end,
})

local noclip = false
local noclipConnection
CheatTab:CreateToggle({
   Name = "NoClip (Wall Pass)",
   CurrentValue = false,
   Callback = function(Value)
      noclip = Value
      if noclip then
         noclipConnection = game:GetService("RunService").Stepped:Connect(function()
            if noclip and game.Players.LocalPlayer.Character then
               for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                  if v:IsA("BasePart") then v.CanCollide = false end
               end
            end
         end)
      else
         if noclipConnection then noclipConnection:Disconnect() end
         if game.Players.LocalPlayer.Character then
            for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
               if v:IsA("BasePart") then v.CanCollide = true end
            end
         end
      end
   end,
})

CheatTab:CreateSection("Stats Modifiers")
local SpeedSlider = CheatTab:CreateSlider({
   Name = "WalkSpeed", Range = {16, 500}, Increment = 1, CurrentValue = 16,
   Callback = function(Value) game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value end,
})

local JumpSlider = CheatTab:CreateSlider({
   Name = "JumpPower", Range = {50, 500}, Increment = 1, CurrentValue = 50,
   Callback = function(Value) game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value end,
})

CheatTab:CreateButton({
   Name = "Reset Stats",
   Callback = function() SpeedSlider:Set(16) JumpSlider:Set(50) end,
})

CheatTab:CreateSection("Performance Optimization")
CheatTab:CreateToggle({
   Name = "FPS Booster",
   CurrentValue = false,
   Callback = function(Value)
      if Value then
         for _,v in pairs(game:GetDescendants()) do
            if v:IsA("BasePart") then v.Material = Enum.Material.Plastic v.Reflectance = 0
            elseif v:IsA("Decal") or v:IsA("Texture") then v:Destroy() end
         end
         game.Lighting.GlobalShadows = false
      end
   end,
})

