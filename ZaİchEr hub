local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "ZaÄ°chEr hub",
   LoadingTitle = "Loading Essential Scripts...",
   LoadingSubtitle = "by peynirimben2",
   KeySystem = false,
})

-- TABS
local MainTab = Window:CreateTab("âš¡ Main", 4483362458)
local ScriptTab = Window:CreateTab("ðŸ“œ Scripts", 4483362458)

--- 1. MAIN HACKS (Ä°stediklerin)
MainTab:CreateSection("Movement")

MainTab:CreateSlider({
   Name = "Speed",
   Range = {16, 500},
   Increment = 1,
   CurrentValue = 16,
   Callback = function(Value) game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value end,
})

MainTab:CreateSlider({
   Name = "Jump Power",
   Range = {50, 500},
   Increment = 1,
   CurrentValue = 50,
   Callback = function(Value) game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value end,
})

MainTab:CreateButton({
   Name = "Fly GUI (E to Toggle)",
   Callback = function() loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.lua"))() end,
})

MainTab:CreateButton({
   Name = "Noclip (Press N)",
   Callback = function()
      local Noclip = nil
      local Clip = nil
      function nclp()
         Clip = false
         local function ncl()
            if Clip == false and game.Players.LocalPlayer.Character ~= nil then
               for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                  if v:IsA('BasePart') and v.CanCollide then v.CanCollide = false end
               end
            end
            wait(0.1)
         end
         Noclip = game:GetService('RunService').Stepped:Connect(ncl)
      end
      nclp()
   end,
})

MainTab:CreateButton({
   Name = "Fling (Kill Others)",
   Callback = function() loadstring(game:HttpGet("https://raw.githubusercontent.com/DigitalityScripts/998877665544332211/main/SimpleFling.lua"))() end,
})

